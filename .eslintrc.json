{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "semi-style": "warn",
        "quotes": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "eqeqeq": "off",
        "no-use-before-define": "off"
    }
}